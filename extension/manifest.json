{
  "manifest_version": 2,
  "name": "SLACKY",
  "description": "Easily share url on Slack",
  "version": "0.0.0.1",
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Slacky",
    "default_popup": "popup.html"
  },
  "permissions": [
    "identity"
  ]
}
